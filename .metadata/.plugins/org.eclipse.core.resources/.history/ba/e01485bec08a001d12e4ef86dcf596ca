package libraryAcessSystem;

import studentEntities.Student;

public class LibrarySystem {
	
	private Student student;
	private LibrarySearchService searchBooksInTheLibrary;
	
	public LibrarySystem() {
	}
	
	public LibrarySystem(Student student, LibrarySearchService searchBooksInTheLibrary) {
		this.student = student;
		this.searchBooksInTheLibrary = searchBooksInTheLibrary;
	}

	public Student getStudent() {
		return student;
	}

	public void setStudent(Student student) {
		this.student = student;
	}

	public LibrarySearchService getSearchBooksInTheLibrary() {
		return searchBooksInTheLibrary;
	}

	public void setSearchBooksInTheLibrary(LibrarySearchService searchBooksInTheLibrary) {
		this.searchBooksInTheLibrary = searchBooksInTheLibrary;
	}
	

}
