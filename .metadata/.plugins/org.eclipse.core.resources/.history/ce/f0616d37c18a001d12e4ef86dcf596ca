package libraryAcessSystem;

import studentEntities.Student;

public class LibrarySystem {
	
	private Student student;
	private LibrarySearchService getLibraryBooks;
	
	// Construtores da classe
	
	public LibrarySystem() {
	}
	
	public LibrarySystem(Student student, LibrarySearchService getLibraryBooks) {
		this.student = student;
		this.searchBooksInTheLibrary = getLibraryBooks;
	}
	
	// Getters e setters

	public Student getStudent() {
		return student;
	}

	public void setStudent(Student student) {
		this.student = student;
	}

	public LibrarySearchService getLibraryBooks() {
		return getLibraryBooks;
	}

	public void setLibraryBooks(LibrarySearchService getLibraryBooks) {
		this.getLibraryBooks = getLibraryBooks;
	}
	

}
